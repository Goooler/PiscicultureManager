<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.goooler.pisciculturemanager.mapper.ManagerMapper">
    <select id="selectSome" resultType="io.goooler.pisciculturemanager.entity.Manager">
        select *
        from manager
        order by `timestamp` desc
        limit #{number}
    </select>

    <select id="selectSomeById" resultType="io.goooler.pisciculturemanager.entity.Manager">
        select *
        from manager
        where id >= #{id}
        order by `timestamp` desc
    </select>
</mapper>
